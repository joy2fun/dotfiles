[core]
    autocrlf = input
    fileMode = false
[user]
    name = joy2fun
    email = web@css.js.cn
[alias]
    co = checkout
    br = branch
    re = remote
    st = status
    ci = commit
    last = log -1 HEAD
    unstage = reset HEAD --
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[merge]
    tool = fugitive
[mergetool]
    prompt = false
    keepBackup = false
[mergetool "fugitive"]
    cmd = vim -f -c \"Gdiff\" \"$MERGED\"
